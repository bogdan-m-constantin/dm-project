            Width="300"/>
        <TextBlock HorizontalAlignment="Left" Margin="1399,511,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="300" TextAlignment="Center" FontSize="24" Height="30" Text="Edge Tracking"/>

